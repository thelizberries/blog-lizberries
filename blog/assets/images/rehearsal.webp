<?xml version="1.0" encoding="UTF-8"?>
<!-- Stylized illustrative band rehearsal placeholder (SVG saved with .webp extension as requested) -->
<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="630" viewBox="0 0 1200 630" role="img" aria-labelledby="title">
  <title id="title">Band rehearsal illustration</title>
  <defs>
    <linearGradient id="bg" x1="0" x2="1">
      <stop offset="0%" stop-color="#0f172a"/>
      <stop offset="100%" stop-color="#1e3a8a"/>
    </linearGradient>
    <filter id="soft" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="12" result="b"/>
      <feBlend in="SourceGraphic" in2="b" />
    </filter>
  </defs>

  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bg)" />

  <!-- Stage floor -->
  <rect x="0" y="430" width="1200" height="200" fill="#0b1220" opacity="0.9"/>
  <rect x="60" y="420" width="1080" height="8" fill="#f59e0b" opacity="0.2"/>

  <!-- Drum kit simplified -->
  <g transform="translate(250,430)">
    <ellipse cx="0" cy="-30" rx="60" ry="30" fill="#ef4444" filter="url(#soft)"/>
    <circle cx="0" cy="-30" r="40" fill="#b91c1c" stroke="#7f1d1d" stroke-width="6"/>
    <rect x="-10" y="-100" width="20" height="80" fill="#111827" transform="rotate(-6)"/>
    <rect x="40" y="-80" width="6" height="40" fill="#111827" transform="rotate(14)"/>
  </g>

  <!-- Electric guitar silhouette -->
  <g transform="translate(750,380) scale(1.2)">
    <path d="M-60,40 C-20,10 20,10 60,40 C80,60 60,100 30,110 C10,120 -20,110 -40,90 C-70,60 -90,50 -60,40 Z" fill="#f97316" stroke="#c2410c" stroke-width="6"/>
    <rect x="58" y="-40" width="14" height="120" rx="7" fill="#111827"/>
    <circle cx="-10" cy="70" r="6" fill="#000"/>
  </g>

  <!-- Microphone stand -->
  <g transform="translate(540,300)">
    <rect x="-4" y="40" width="8" height="200" fill="#111827"/>
    <circle cx="0" cy="28" r="18" fill="#e5e7eb" stroke="#9ca3af" stroke-width="3"/>
    <rect x="20" y="20" width="120" height="6" rx="3" fill="#111827" transform="rotate(-12)"/>
  </g>

  <!-- Amplifier -->
  <g transform="translate(920,420)">
    <rect x="-80" y="-80" width="160" height="80" rx="8" fill="#111827"/>
    <rect x="-72" y="-72" width="144" height="64" fill="#374151"/>
    <circle cx="-30" cy="-40" r="10" fill="#f59e0b"/>
    <circle cx="30" cy="-40" r="10" fill="#f59e0b"/>
  </g>

  <!-- Audience silhouettes (minimal) -->
  <g fill="#0b1220" opacity="0.6">
    <ellipse cx="200" cy="520" rx="40" ry="12"/>
    <ellipse cx="300" cy="520" rx="35" ry="10"/>
    <ellipse cx="400" cy="520" rx="45" ry="13"/>
    <ellipse cx="600" cy="520" rx="60" ry="16"/>
    <ellipse cx="800" cy="520" rx="38" ry="11"/>
  </g>

  <!-- Stage lights -->
  <g opacity="0.45">
    <ellipse cx="320" cy="120" rx="220" ry="80" fill="#fde68a"/>
    <ellipse cx="880" cy="120" rx="220" ry="80" fill="#bfdbfe"/>
  </g>

  <!-- Center label -->
  <g>
    <rect x="420" y="480" width="360" height="70" rx="12" fill="#ffffff" opacity="0.06" />
    <text x="600" y="528" font-family="Inter, Roboto, Arial, sans-serif" font-size="36" fill="#ffffff" text-anchor="middle" font-weight="600">Rehearsal</text>
    <text x="600" y="560" font-family="Inter, Roboto, Arial, sans-serif" font-size="16" fill="#e5e7eb" text-anchor="middle">Music band rehearsal â€” stylized placeholder</text>
  </g>

  <!-- Decorative musical notes -->
  <g fill="#fef3c7" opacity="0.9">
    <path d="M980 90c0-6 4-11 10-11s10 5 10 11-4 11-10 11c-6 0-10-5-10-11z"/>
    <rect x="992" y="80" width="4" height="36" rx="2"/>
    <path d="M1020 60c0-6 4-11 10-11s10 5 10 11-4 11-10 11c-6 0-10-5-10-11z"/>
    <rect x="1032" y="50" width="4" height="36" rx="2"/>
  </g>

</svg>
